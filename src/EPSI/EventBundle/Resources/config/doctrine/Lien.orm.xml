<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EPSI\EventBundle\Entity\Lien" table="LIEN">
    <indexes>
      <index name="FK_LIEN_id_lieu" columns="id_lieu"/>
      <index name="FK_LIEN_id_evenement" columns="id_evenement"/>
      <index name="FK_LIEN_id_artiste" columns="id_artiste"/>
      <index name="FK_LIEN_id_type_lien" columns="id_type_lien"/>
    </indexes>
    <id name="idLien" type="integer" column="id_lien">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lien" type="string" column="lien" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nbVisiteLien" type="integer" column="nb_visite_lien" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idTypeLien" target-entity="TypeLien" fetch="LAZY">
      <join-columns>
        <join-column name="id_type_lien" referenced-column-name="id_type_lien"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idLieu" target-entity="Lieu" fetch="LAZY">
      <join-columns>
        <join-column name="id_lieu" referenced-column-name="id_lieu"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEvenement" target-entity="Evenement" fetch="LAZY">
      <join-columns>
        <join-column name="id_evenement" referenced-column-name="id_evenement"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idArtiste" target-entity="Artiste" fetch="LAZY">
      <join-columns>
        <join-column name="id_artiste" referenced-column-name="id_artiste"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
