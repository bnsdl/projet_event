<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EPSI\EventBundle\Entity\Lieu" table="LIEU">
    <id name="idLieu" type="integer" column="id_lieu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomLieu" type="string" column="nom_lieu" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="5000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="252" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codePostal" type="string" column="code_postal" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="252" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lienImage" type="string" column="lien_image" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
