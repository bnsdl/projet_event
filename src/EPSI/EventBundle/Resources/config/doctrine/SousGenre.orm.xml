<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EPSI\EventBundle\Entity\SousGenre" table="SOUS_GENRE">
    <indexes>
      <index name="FK_SOUS_GENRE_id_genre_GENRE" columns="id_genre_GENRE"/>
    </indexes>
    <id name="idGenre" type="integer" column="id_genre">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomSousGenre" type="string" column="nom_sous_genre" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nbVisiteSousGenre" type="integer" column="nb_visite_sous_genre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idGenreGenre" target-entity="Genre" fetch="LAZY">
      <join-columns>
        <join-column name="id_genre_GENRE" referenced-column-name="id_genre"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
